class Solution {
    public boolean isPalindrome(int  n) {
      
        int a=n,sum=0,temp;
        while ( n> 0) {
           temp=n%10;
            sum=(sum*10)+temp;
            n = n/ 10;
        }
        if (sum==a) 
        
    return true;
        else
             
        return false;
    }
    }
